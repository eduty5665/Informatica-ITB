import java.util.Scanner;
public class ExerCamila {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner tcd = new Scanner(System.in);
		int a = tcd.nextInt();
		int b = tcd.nextInt();
		int c = tcd.nextInt();
		int num1 = 0; 
		int num2 = 0; 
		int num3 = 0; 
		
		if(a > b && a > c){
			num1 = a;
			if(b > c) {
				num2 = b;
				num3 = c;
			}else (c > b){
				num2 = c;
				num3 = b;
			}
		
		
 
		System.out.println(num2);
	}

}
